{
  "name": "supertag-cli",
  "version": "1.3.4",
  "description": "CLI for Tana integration - query, create, sync, and manage Tana workspaces with semantic search",
  "type": "module",
  "bin": {
    "supertag": "./supertag"
  },
  "scripts": {
    "build": "bun build src/index.ts --compile --outfile=supertag",
    "build:mcp": "bun build src/mcp/index.ts --compile --outfile=supertag-mcp",
    "dev": "bun run src/index.ts",
    "dev:mcp": "bun run src/mcp/index.ts",
    "inspect:mcp": "npx @modelcontextprotocol/inspector bun run src/mcp/index.ts",
    "test": "bun test ./tests/*.test.ts ./tests/codegen/ ./tests/commands/ ./tests/config/ ./tests/db/ ./tests/e2e/ ./tests/embeddings/ ./tests/integration/ ./tests/mcp/ ./tests/services/ ./tests/types/ ./tests/utils/ ./tests/visualization/ ./src/**/*.test.ts",
    "test:slow": "bun test ./tests/slow/",
    "test:full": "bun test",
    "typecheck": "tsc --noEmit",
    "precommit": "bun run test:full"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "commander": "^12.0.0",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "fastest-levenshtein": "^1.0.16",
    "fastify": "^5.6.2",
    "resona": "github:jcfischer/resona#main",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "playwright": "^1.57.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "tana",
    "knowledge-management",
    "cli",
    "typescript",
    "bun",
    "sqlite",
    "mcp",
    "semantic-search"
  ],
  "author": "Jens-Christian Fischer <jens-christian@invisible.ch>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcfischer/supertag-cli.git"
  },
  "bugs": {
    "url": "https://github.com/jcfischer/supertag-cli/issues"
  },
  "homepage": "https://github.com/jcfischer/supertag-cli#readme"
}
